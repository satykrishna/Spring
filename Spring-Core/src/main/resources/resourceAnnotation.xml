<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<bean id="fileWriter" name="fileWriter"
		class="example.Spring.autowire.annotation.resourceAnnotation.MyFileWriter">
	</bean>
	<bean id="consoleWriter" name="consoleWriter"
		class="example.Spring.autowire.annotation.resourceAnnotation.MyConsoleWriter">
	</bean>

	<bean id="consoleWriterDup" name="consoleWriterDup"
		class="example.Spring.autowire.annotation.resourceAnnotation.MyConsoleWriter">
	</bean>

	<bean id="fileWriterDup" name="fileWriterDup"
		class="example.Spring.autowire.annotation.resourceAnnotation.MyFileWriter">
	</bean>

	<bean id="myLogger" name="myLogger"
		class="example.Spring.autowire.annotation.resourceAnnotation.MyLogger" />


	<context:annotation-config />
</beans>
